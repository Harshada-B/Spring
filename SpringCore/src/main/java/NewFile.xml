<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean class="com.yash.SpringCore.Employee" name="Employee1">
<property name="E_id">
<value>1022</value>
</property>

<!--USING VALUE AS ATTRIBUTE-->
<property name="E_name" value="Harshada Buchude" />

<property name="E_city">
<value>pune</value>
</property>
</bean>

<!-- USING P SCHEMA -->

<bean class="com.yash.SpringCore.Employee" name="Employee2" p:E_id="201" p:E_name="Aditi" p:E_city="rajgurunagar"/>
</beans>  